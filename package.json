{
  "name": "warframe-items",
  "version": "0.0.0-development",
  "description": "Get all warframe items directly from Warframe's API.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "node ./build/build.js",
    "test": "nyc mocha",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "prepublishOnly": "npm_config_yes=true npx clean-package",
    "pretypings": "typescript-json-schema ./index.d.ts Items --out items.schema.json --noExtraProps --required --strictNullChecks",
    "typings": "ajv validate -s ./items.schema.json -d ./data/json/All.json --all-errors --allowUnionTypes --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wfcd/warframe-items.git"
  },
  "author": "Kaptard",
  "contributors": [
    "Tobiah <tobiah@pm.me>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wfcd/warframe-items/issues"
  },
  "homepage": "https://github.com/wfcd/warframe-items#readme",
  "devDependencies": {
    "@types/imagemin": "^8.0.0",
    "@types/imagemin-jpegtran": "^5.0.1",
    "@types/node-fetch": "^2.5.12",
    "@types/sharp": "^0.29.1",
    "ajv-cli": "^5.0.0",
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.3.3",
    "decache": "^4.6.0",
    "eslint": "^8.2.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "fs-extra": "^10.0.0",
    "get-image-colors": "^4.0.0",
    "image-downloader": "^4.0.3",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "lodash": "^4.17.19",
    "lua.vm.js": "0.0.1",
    "lzma": "^2.3.2",
    "mocha": "^9.0.2",
    "node-cmd": "^4.0.0",
    "node-fetch": "^2.6.1",
    "node-progress": "^0.1.0",
    "nyc": "^15.1.0",
    "sharp": "^0.28.3",
    "socks5-http-client": "^1.0.4",
    "typescript-json-schema": "^0.51.0",
    "warframe-patchlogs": "^1.661.0"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "globals": {
      "describe": "readonly",
      "it": "readonly",
      "beforeEach": "readonly",
      "afterEach": "readonly"
    },
    "rules": {
      "brace-style": 0
    },
    "parserOptions": {
      "ecmaVersion": 9
    },
    "env": {
      "es6": true
    }
  },
  "mocha": {
    "slow": 10000,
    "timeout": 10000
  },
  "clean-package": {
    "remove": [
      "mocha",
      "devDependencies",
      "eslintIgnore",
      "eslintConfig",
      "scripts",
      "clean-package",
      "nyc"
    ]
  },
  "nyc": {
    "exclude": [
      "test/*",
      "build/*",
      "config/*",
      "data/*",
      "docker/*",
      "index.d.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "skip-full": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "check-coverage": true
  }
}
