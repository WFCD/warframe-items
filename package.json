{
  "name": "warframe-items",
  "version": "0.0.0-development",
  "description": "Get all warframe items directly from Warframe's API.",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "scripts": {
    "build": "node ./build/build.js",
    "test": "nyc mocha --exclude test/*.regression.spec.mjs",
    "regression": "mocha --include test/index.regression.spec.mjs",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "postinstall": "install-peerdeps @wfcd/eslint-config@^1.1.4 -S",
    "prepublishOnly": "npm_config_yes=true npx clean-package",
    "pretypings": "typescript-json-schema ./index.d.ts Items --out items.schema.json --noExtraProps --required --strictNullChecks",
    "typings": "ajv validate -s ./items.schema.json -d ./data/json/All.json --all-errors --allowUnionTypes --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wfcd/warframe-items.git"
  },
  "author": "Kaptard",
  "contributors": [
    "Tobiah <tobiah@pm.me>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wfcd/warframe-items/issues"
  },
  "homepage": "https://github.com/wfcd/warframe-items#readme",
  "devDependencies": {
    "@types/imagemin": "^8.0.0",
    "@types/imagemin-jpegtran": "^5.0.1",
    "@types/node-fetch": "^2.5.12",
    "@types/sharp": "^0.30.0",
    "@wfcd/eslint-config": "^1.1.5",
    "ajv-cli": "^5.0.0",
    "cheerio": "^1.0.0-rc.3",
    "colors": "1.4.0",
    "decache": "^4.6.0",
    "get-image-colors": "^4.0.0",
    "image-downloader": "^4.0.3",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "install-peerdeps": "^2.0.3",
    "lodash": "^4.17.19",
    "lua.vm.js": "0.0.1",
    "lzma": "^2.3.2",
    "mocha": "^10.0.0",
    "node-cmd": "^4.0.0",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "progress": "^2.0.3",
    "sharp": "^0.30.2",
    "socks5-http-client": "^1.0.4",
    "tslib": "^2.4.0",
    "typescript-json-schema": "^0.54.0",
    "warframe-patchlogs": "^1.737.0"
  },
  "eslintConfig": {
    "extends": [
      "@wfcd"
    ]
  },
  "prettier": "@wfcd/eslint-config/prettier",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-private-methods"
    ]
  },
  "mocha": {
    "slow": 10000,
    "timeout": 10000
  },
  "clean-package": {
    "remove": [
      "mocha",
      "devDependencies",
      "eslintIgnore",
      "eslintConfig",
      "scripts",
      "clean-package",
      "nyc",
      "babel",
      "prettier"
    ]
  },
  "nyc": {
    "exclude": [
      "test/*",
      "build/*",
      "config/*",
      "data/*",
      "docker/*",
      "index.d.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "skip-full": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "check-coverage": true
  }
}
